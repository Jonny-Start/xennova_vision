{
  "camera": {
    "type": "usb",
    "device_id": 0,
    "resolution": [
      1920,
      1080
    ],
    "fps": 30
  },
  "ai_model": {
    "type": "advanced",
    "ocr_engine": "easyocr",
    "yolo_model": "yolov8n.pt",
    "confidence_threshold": 0.8
  },
  "storage": {
    "type": "sqlite",
    "database_path": "./plate_events.db",
    "max_events": 10000
  },
  "network": {
    "endpoint": "https://your-api-endpoint.com/plate-events",
    "timeout": 10,
    "retry_attempts": 5,
    "batch_size": 50
  },
  "capture_interval": 0.5,
  "log_level": "DEBUG"
}